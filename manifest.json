{
  "name": "Lightspeed",
  "version": "0.1",
  "description": "Lightspeed is an experimental Chrome extension which serves as an alternative to native bookmarking.",
  "homepage_url": "http://github.com/apwit/lightspeed",
  "options_page": "html/options.html",
  "background_page": "html/background.html",
  "permissions": ["experimental", "tabs"],
  "page_action": {
    "default_title": "Bookmark this page with Lightspeed",
    "default_popup": "html/popup.html"
  },
  "icons": {
    "16": "images/icons/16.png",
    "48": "images/icons/48.png",
    "128": "images/icons/128.png"
  }
}
